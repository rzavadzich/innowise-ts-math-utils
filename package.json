{
  "name": "@rzavadzich/innowise-ts-math-utils",
  "version": "0.0.0",
  "description": "TypeScript math utilities with full CI/CD pipeline",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run tests/e2e",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "typescript",
    "math",
    "utils",
    "ci-cd"
  ],
  "author": "ruslan.zavadzich <ruslan.zavadzich@innowise.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rzavadzich/innowise-ts-math-utils.git"
  },
  "bugs": {
    "url": "https://github.com/rzavadzich/innowise-ts-math-utils/issues"
  },
  "homepage": "https://github.com/rzavadzich/innowise-ts-math-utils#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.9",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  }
}
